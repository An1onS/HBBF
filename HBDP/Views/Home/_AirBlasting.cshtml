@model HBDP.Models.Input.AirBlasting

<h4>AirBlasting</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="_AirBlasting">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Temperature" class="control-label"></label>
                <input asp-for="Temperature" class="form-control" />
                <span asp-validation-for="Temperature" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Dampness" class="control-label"></label>
                <input asp-for="Dampness" class="form-control" />
                <span asp-validation-for="Dampness" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OxygenPercent" class="control-label"></label>
                <input asp-for="OxygenPercent" class="form-control" />
                <span asp-validation-for="OxygenPercent" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Consumption" class="control-label"></label>
                <input asp-for="Consumption" class="form-control" />
                <span asp-validation-for="Consumption" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Methane" class="control-label"></label>
                <input asp-for="Methane" class="form-control" />
                <span asp-validation-for="Methane" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ethane" class="control-label"></label>
                <input asp-for="Ethane" class="form-control" />
                <span asp-validation-for="Ethane" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CarbonDioxide" class="control-label"></label>
                <input asp-for="CarbonDioxide" class="form-control" />
                <span asp-validation-for="CarbonDioxide" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Carbon" class="control-label"></label>
                <input asp-for="Carbon" class="form-control" />
                <span asp-validation-for="Carbon" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Hydrogen" class="control-label"></label>
                <input asp-for="Hydrogen" class="form-control" />
                <span asp-validation-for="Hydrogen" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
